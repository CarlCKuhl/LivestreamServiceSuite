<Window x:Class="Presenter.PresenterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presenter"
        mc:Ignorable="d"
        KeyDown="Window_KeyDown"
        Title="Presenter" Height="450" Width="800">
    <Grid>
        <MediaElement Name="videoPlayer" LoadedBehavior="Manual" UnloadedBehavior="Close"></MediaElement>
        <Image x:Name="imagePlayer"></Image>
    </Grid>
</Window>
