<Window x:Class="IntegratedPresenter.Main.ConnectComPort"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IntegratedPresenter.Main"
        mc:Ignorable="d"
        Title="Connect" Height="150" Width="230">
    <Grid>
        <StackPanel>
            <ComboBox Margin="20" Name="cbportnames" Width="160"></ComboBox>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Button Margin="10" Padding="10" Click="ClickConnect">Connect</Button>
                <Button Margin="10" Padding="10" Click="ClickCancel">Cancel</Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
